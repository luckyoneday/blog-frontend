{
  "name": "blog-frontend",
  "version": "1.0.0",
  "description": "react blog frontend",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:client": "cross-env NODE_ENV=production webpack --config config/webpack.config.client.js",
    "build:server": "cross-env NODE_ENV=production webpack --config config/webpack.config.server.js",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "dev:server": "rimraf dist/server.bundle.js && cross-env NODE_ENV=development webpack --config ./config/webpack.config.server.js --watch ",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server --config ./config/webpack.config.client.js",
    "dev": "npm run dev:client && npm run dev:server",
    "start": "npm run build && node dist/server.bundle.js"
  },
  "keywords": [
    "react",
    "typescript"
  ],
  "author": "youting",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/luckyoneday/blog-frontend/issues"
  },
  "homepage": "https://github.com/luckyoneday/blog-frontend#readme",
